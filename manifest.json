{
  "name": "Hearthstone Library",
  "description": "Enhance r/hearthstone",
  "version": "0.1",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.reddit.com/r/hearthstone/comments/*"
      ],
      "js": [
        "underscore.js",
        "HSLParser.js",
        "HSLModel.js",
        "HSLServices.js",
        "HSL"
        "HSLibrary.js"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
